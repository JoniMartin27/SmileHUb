	<queryString language="SQL">
		<![CDATA[SELECT concat(consulta_cita.fecha,' ',consulta_cita.hora) as "Fecha y hora",tratamiento.nombre_tratamiento as "Tratamiento",
paciente.nombre as "Nombre Paciente",consulta_cita.observaciones,doctor_administrador.nombre as "Nombre Doctor"
from consulta_cita
inner join tratamiento on consulta_cita.id_tratamiento=tratamiento.id_tratamiento
inner join doctor_administrador on consulta_cita.id_doctor=doctor_administrador.id_doctor_administrador
inner join historial on consulta_cita.id_historial=historial.id_historial
inner join paciente on consulta_cita.id_paciente=paciente.id_paciente

where  consulta_cita.id_paciente=$P{id_paciente}]]>
	</queryString>
